  "bugs": {
    "url": "https://github.com/aardpro/time-id/issues"
  },
  "homepage": "https://github.com/aardpro/time-id#readme",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10"
  },
  "type": "module",
